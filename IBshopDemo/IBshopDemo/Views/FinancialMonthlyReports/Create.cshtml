@model IBshopDemo.Models.FinancialMonthlyReport

@{
    ViewData["Title"] = "Create";
}


<div class="content-wrapper">
    <!-- Content -->

    <div class="container-xxl flex-grow-1 container-p-y">
        <h4 class="py-3 mb-4">
            <span class="text-muted fw-light">Forms/</span>
            Sticky Actions
        </h4>
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div id="sticky-wrapper" class="sticky-wrapper" style="height: 86.9375px;">
                        <div class="card-header sticky-element bg-label-secondary d-flex justify-content-sm-between align-items-sm-center flex-column flex-sm-row" style="">
                            <h5 class="card-title mb-sm-0 me-2">Sticky Action Bar</h5>
                            <div class="action-btns">
                                <a asp-action="Index">
                                    <button class="btn btn-label-primary me-3">
                                        <span class="align-middle">بازگشت به لیست</span>
                                    </button>
                                </a>

                                <button type="submit" class="btn btn-primary">
                                    ثبت
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-lg-8 mx-auto">
                                <form asp-controller="FinancialMonthlyReports" asp-action="Create" method="form">
                                    <div asp-validation-summary="ModelOnly" class="text-danger col-12"></div>

                                    <div class="row">
                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="FinancialMrid" class="form-label mt-3"></label>
                                            <input asp-for="FinancialMrid" class="form-control mt-1 shadow-sm" />
                                            <span asp-validation-for="FinancialMrid" class="text-danger"></span>
                                        </div>
                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="Year" class="form-label mt-3"></label>
                                            <input asp-for="Year" class="form-control mt-1 shadow-sm" />
                                            <span asp-validation-for="Year" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="Month" class="form-label mt-3"></label>
                                            <input asp-for="Month" class="form-control mt-1 shadow-sm" />
                                            <span asp-validation-for="Month" class="text-danger"></span>
                                        </div>
                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="MonthNumber" class="form-label mt-3"></label>
                                            <input asp-for="MonthNumber" class="form-control mt-1 shadow-sm" />
                                            <span asp-validation-for="MonthNumber" class="text-danger"></span>
                                        </div>
