                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="PrpReqQty" class="form-label mt-3"></label>
                                            <input asp-for="PrpReqQty" class="form-control mt-1 shadow-sm" />
                                            <span asp-validation-for="PrpReqQty" class="text-danger"></span>
                                        </div>
                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="MtnprpQty" class="form-label mt-3"></label>
