                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="LblPrpQty" class="form-label mt-3"></label>
                                            <input asp-for="LblPrpQty" class="form-control mt-1 shadow-sm" />
                                            <span asp-validation-for="LblPrpQty" class="text-danger"></span>
                                        </div>
                                    </div>

                                    
                                </form>

                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

@section Scripts {
    <script src="~/theme/assets/vendor/libs/jquery-sticky/jquery-sticky.js"></script>
    <script src="~/theme/assets/js/form-layouts.js"></script>

}









