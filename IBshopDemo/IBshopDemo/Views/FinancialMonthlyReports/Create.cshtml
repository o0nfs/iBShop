                                        </div>
                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="TaminInstallmentQty" class="form-label mt-3"></label>
                                            <input asp-for="TaminInstallmentQty" class="form-control mt-1 shadow-sm" />
                                            <span asp-validation-for="TaminInstallmentQty" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="TaminPenaltyVol" class="form-label mt-3"></label>
                                            <input asp-for="TaminPenaltyVol" class="form-control mt-1 shadow-sm" />
