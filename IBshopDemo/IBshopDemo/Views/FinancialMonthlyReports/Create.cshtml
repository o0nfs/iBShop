                                            <span asp-validation-for="TreasuryBillsQty" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="Recivedgaurantee" class="form-label mt-3"></label>
                                            <input asp-for="Recivedgaurantee" class="form-control mt-1 shadow-sm" />
                                            <span asp-validation-for="Recivedgaurantee" class="text-danger"></span>
                                        </div>
                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="RestaxinstallmentQty" class="form-label mt-3"></label>
                                            <input asp-for="RestaxinstallmentQty" class="form-control mt-1 shadow-sm" />
                                            <span asp-validation-for="RestaxinstallmentQty" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="ResFundInstallmenQty" class="form-label mt-3"></label>
                                            <input asp-for="ResFundInstallmenQty" class="form-control mt-1 shadow-sm" />
