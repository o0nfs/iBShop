                                        </div>
                                        <div class="form-group col-6 mt-1">
                                            <label asp-for="CheckedfundQty" class="form-label mt-3"></label>
                                            <input asp-for="CheckedfundQty" class="form-control mt-1 shadow-sm" />
                                            <span asp-validation-for="CheckedfundQty" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group col-6 mt-1">
