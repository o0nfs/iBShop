                @Html.DisplayNameFor(model => model.GoharRev)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalIssue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalRev)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BrancheCodeNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IncomeCenter)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MonthNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MonthName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServatIssue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServatRev)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GoharIssue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GoharRev)
