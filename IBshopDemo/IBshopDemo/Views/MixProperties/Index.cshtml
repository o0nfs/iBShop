            <td>
                @Html.DisplayFor(modelItem => item.TotalIssue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalRev)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BrancheCodeNavigation.BranchCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IncomeCenter.IncomecenterId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MixIncomeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MixIncomeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MixIncomeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
