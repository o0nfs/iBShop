@model IBshopDemo.Models.ProductDevelopmentMontylyReport

@{
    ViewData["Title"] = "Create";
}
<style>
    div.card {
        border-bottom: 3px solid green;
        border-top: 3px solid green;
        border-radius: 1.25rem !important;
    }

    .btn-create {
        font-size: 15px;
    }

</style>
<div class="row  p-3">
    <div class="row p-3">
        <h1>Create</h1>
        <h4>ProductDevelopmentMontylyReport</h4>
    </div>
    <hr />
    <div class="col-1"></div>
    <div class="col-10">
        <div class="card px-4 mt-3 shadow bg-white " style="direction:rtl">
            <div class="card-body  pt-5">
               
                <form asp-action="Create">
                    <div asp-validation-summary="ModelOnly" class="text-danger col-12"></div>
					<div class="row">
						<div class="form-group col-6 mt-1">
							<label asp-for="DevMrid" class="form-label mt-3"></label>
							<input asp-for="DevMrid" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="DevMrid" class="text-danger"></span>
						</div>
						<div class="form-group col-6 mt-1">
							<label asp-for="Year" class="form-label mt-3"></label>
							<input asp-for="Year" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="Year" class="text-danger"></span>
						</div>
					</div>
					<div class="row">
						<div class="form-group col-6 mt-1">
							<label asp-for="Month" class="form-label mt-3"></label>
							<input asp-for="Month" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="Month" class="text-danger"></span>
						</div>
						<div class="form-group col-6 mt-1">
							<label asp-for="MonthNumber" class="form-label mt-3"></label>
							<input asp-for="MonthNumber" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="MonthNumber" class="text-danger"></span>
						</div>
					</div>
					<div class="row">
						<div class="form-group col-6 mt-1">
							<label asp-for="CheckedSuggestQty" class="form-label mt-3"></label>
							<input asp-for="CheckedSuggestQty" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="CheckedSuggestQty" class="text-danger"></span>
						</div>
						<div class="form-group col-6 mt-1">
							<label asp-for="AcceptedSuggestQty" class="form-label mt-3"></label>
							<input asp-for="AcceptedSuggestQty" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="AcceptedSuggestQty" class="text-danger"></span>
						</div>
					</div>
					<div class="row">
						<div class="form-group col-6 mt-1">
							<label asp-for="SuggestPersonnelQty" class="form-label mt-3"></label>
							<input asp-for="SuggestPersonnelQty" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="SuggestPersonnelQty" class="text-danger"></span>
						</div>
						<div class="form-group col-6 mt-1">
							<label asp-for="EconomicalSuggestQty" class="form-label mt-3"></label>
							<input asp-for="EconomicalSuggestQty" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="EconomicalSuggestQty" class="text-danger"></span>
						</div>
					</div>
					<div class="row">
						<div class="form-group col-6 mt-1">
							<label asp-for="IbcCreaditCardCustQty" class="form-label mt-3"></label>
							<input asp-for="IbcCreaditCardCustQty" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="IbcCreaditCardCustQty" class="text-danger"></span>
						</div>
						<div class="form-group col-6 mt-1">
							<label asp-for="IbcComplQty" class="form-label mt-3"></label>
							<input asp-for="IbcComplQty" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="IbcComplQty" class="text-danger"></span>
						</div>
					</div>
					<div class="row">
						<div class="form-group col-6 mt-1">
							<label asp-for="IbcCreadiCardReqQty" class="form-label mt-3"></label>
							<input asp-for="IbcCreadiCardReqQty" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="IbcCreadiCardReqQty" class="text-danger"></span>
						</div>
						<div class="form-group col-6 mt-1">
							<label asp-for="IbcCreaditCardProcessAvgTime" class="form-label mt-3"></label>
							<input asp-for="IbcCreaditCardProcessAvgTime" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="IbcCreaditCardProcessAvgTime" class="text-danger"></span>
						</div>
					</div>
					<div class="row">
						<div class="form-group col-6 mt-1">
							<label asp-for="IbwDesignQty" class="form-label mt-3"></label>
							<input asp-for="IbwDesignQty" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="IbwDesignQty" class="text-danger"></span>
						</div>
						<div class="form-group col-6 mt-1">
							<label asp-for="IbwDesignVol" class="form-label mt-3"></label>
							<input asp-for="IbwDesignVol" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="IbwDesignVol" class="text-danger"></span>
						</div>
					</div>
					<div class="row">
						<div class="form-group col-6 mt-1">
							<label asp-for="IbwDesignTime" class="form-label mt-3"></label>
							<input asp-for="IbwDesignTime" class="form-control mt-1 shadow-sm" />
							<span asp-validation-for="IbwDesignTime" class="text-danger"></span>
						</div>
					</div>


					<div class="row m-5 pb-5">
						<div class="form-group">
							<a asp-action="Index" class="mt-4 me-3 float-end ">بازگشت به لیست</a>
							<input type="submit" value="ثبت" class=" py-3 px-5 float-start btn btn-outline-success me-3 btn-create" />
						</div>
					</div>
                </form>
            </div>
        </div>

    </div>
    <div class="col-1"></div>
</div>


      

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
