                                    @Html.DisplayNameFor(model => model.CrmcommHold)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.CrmdoneCmm)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.CrmfailCmm)
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Year)
                                    </td>
                                    <td>
