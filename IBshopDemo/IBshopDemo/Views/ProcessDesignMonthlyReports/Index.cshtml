                                        <a asp-action="Details" asp-route-id="@item.ProcessDesignMrid">Details</a> |
                                        <a asp-action="Delete" asp-route-id="@item.ProcessDesignMrid">Delete</a>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>

        </div>
    </div>
</div>
